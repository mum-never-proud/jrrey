{
	"presets": [
		"@babel/preset-env",
		"@babel/preset-react"
	],
	"plugins": [
    ["module-resolver", {
      "alias": {
				"jrrey": "./dist/jrrey.js",
				"utils": "./jrrey-demo-app/utils",
				"components": "./jrrey-demo-app/components",
				"images": "./jrrey-demo-app/public/images",
				"constants": "./jrrey-demo-app/constants"
			},
			"extensions": [".js", ".jsx"]
		}],
		["@babel/plugin-transform-react-jsx", {
			"pragma": "h",
			"pragmaFrag": "\"div\""
		}]
  ]
}
